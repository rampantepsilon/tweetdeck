{
  "name": "RampantDock",
  "version": "4.0.0-beta5",
  "description": "Multitasking at it's finest! Introducing our savior RampantDock!",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "test": "electron .",
    "package-mac": "electron-packager ./ --overwrite --platform=darwin --asar --arch=x64 --icon=logo.png --out=build",
    "package-win64": "electron-packager ./ rampantdock --overwrite --asar --platform=win32 --arch=x64 --icon=logo.ico --out=build --version-string.CompanyName=RampantEpsilon --version-string.ProductName=\"RampantDock\"",
    "package-win32": "electron-packager ./ rampantdock --overwrite --asar --platform=win32 --arch=ia32 --icon=logo.ico --out=build --version-string.CompanyName=RampantEpsilon --version-string.ProductName=\"RampantDock\"",
    "package-linux": "electron-packager ./ rampantdock --platform=linux --arch=x64 --overwrite --asar  --icon=logo.png --out=build"
  },
  "author": "Tom \"RampantEpsilon\" Ware",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2"
  },
  "devDependencies": {
    "electron": "^9.4.0",
    "jquery": "^3.5.1",
    "xhr2": "^0.2.0"
  }
}
